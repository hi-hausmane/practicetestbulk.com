# Supabase Authentication
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_service_role_key_here
SUPABASE_ANON_KEY=your_anon_public_key_here
SUPABASE_JWT_SECRET=your_jwt_secret_here

# Google OAuth (configured in Supabase Dashboard → Authentication → Providers)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Email Configuration (SMTP via Resend)
# NOTE: SMTP settings are configured in Supabase Dashboard → Project Settings → Auth → SMTP Settings
# These variables are for documentation only - actual SMTP is configured in Supabase UI
SMTP_HOST=smtp.resend.com
SMTP_PORT=465
SMTP_USER=resend
SMTP_FROM=noreply@practicetestbulk.com
SMTP_FROM_NAME=PracticeTestBulk
# SMTP_PASSWORD is your Resend API key - configure in Supabase Dashboard (do not store here)

# Stripe Billing
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_PRICE_ID_PRO=price_your_pro_plan_price_id
STRIPE_PRICE_ID_BUSINESS=price_your_business_plan_price_id
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_signing_secret

# Anthropic API for AI Question Generation
ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key_here

